<form id="registForm" class="form-horizontal" method="post">
    <input id="type" type="hidden" name="type" value="normal" />
    <div class="form-group">
        <label for="email" class="col-lg-2 control-label">Email</label>

        <div class="col-lg-10">
            <input type="email" class="form-control" name="email" value="<%= email %>" placeholder="Email" required="true" readOnly>
        </div>
    </div>
    <div class="form-group">
        <label for="name" class="col-lg-2 control-label">name</label>

        <div class="col-lg-10">
            <input type="text" class="form-control" name="name" placeholder="name" value="<%= username %>" minlength="3" maxlength="20" required="true">
        </div>
    </div>
    <div class="form-group">
        <label for="password" class="col-lg-2 control-label">Password</label>

        <div class="col-lg-10">
            <input type="password" class="form-control" name="password" placeholder="Password" value="" minlength="4" maxlength="20" required="true">
        </div>
    </div>
    <div class="form-group">
        <label for="password" class="col-lg-2 control-label">Password check</label>

        <div class="col-lg-10">
            <input type="password" class="form-control" name="passwordChk" placeholder="Password check" value="" minlength="4" maxlength="20" required="true">
        </div>
    </div>
    <div class="form-group">
        <div class="col-lg-offset-2 col-lg-10">
            <div class="checkbox">
                <label>
                    <input name="remember" type="checkbox"> Remember me
                </label>
            </div>
        </div>
    </div>
    <div class="form-group">
        <div class="col-lg-offset-2 col-lg-10">
            <button type="submit" class="btn btn-primary" data-bypass="true">확인</button>
            <a href="#" class="btn btn-default">취소</a>
        </div>
    </div>
</form>

<script>
  document.getElementById("registForm").action=window.location.origin + "/user/";
</script>